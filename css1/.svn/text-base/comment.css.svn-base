}
#loading{
	position: fixed;
    top: 0;
    left: 0px;
    right: 0px;
    bottom: 0px;
    background: rgba(0,0,0,0.1);
    z-index: 9999;
    display:none;
}
#loading.hover{
	display:block !important;
}
#loading img{
	position: fixed;
    top: 43%;
    left: 45%;
}
