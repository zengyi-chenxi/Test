}


/*星级点亮*/
.block {
	width: 80%;
	line-height: 20px;
	overflow: hidden;
}
.score{
	display: none;
}
.star_score {
	/*width: 160px;*/
	height: 20px;
	position: relative;
	float: left;
	margin-left: 5px;
}
.block .attitude {
	font-size: 1.5rem;
	color: #2B2B2B;
	padding-left: 20px;
	line-height: 20px;
	height: 20px;
	float: left;
	margin-top: 2px;
}
.star_score a {
	height: 20px;
	display: block;
	text-indent: -999em;
	position: absolute;
	left: 0;
}
.star_score a.clibg {
	background: url(../img1/red-start.png) 0 -0px;
	background-size: 20px 20px;
	left: 0;
}
#starttwo .star_score,#starttwo1 .star_score,#starttwo2 .star_score {
	background: url(../img1/no-start.png);
	background-size: 20px 20px;
}
#starttwo .star_score a:hover,#starttwo1 .star_score a:hover, #starttwo2 .star_score a:hover{
	background: url(../img1/red-start.png) 0 -0px;
	background-size: 20px 20px;
	left: 0;
}
