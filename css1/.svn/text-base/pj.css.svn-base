#file {
	width: 80px;height: 80px;
	opacity: 0;
	z-index: 99;
}

.img_tx{
	width: 100%;height: auto !important;
	max-height: 100% !important;
	text-align: center;
	overflow: hidden;
	border-radius: initial !important;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	left: 0;
}
#loading{
	position: fixed;
    top: 0;
    left: 0px;
    right: 0px;
    bottom: 0px;
    background: rgba(0,0,0,0.1);
    z-index: 9999;
    display:none;
}
#loading.hover{
	display:block;
}
#loading img{
	position: fixed;
    top: 43%;
    left: 45%;
}
.NoInfoDiv{
	font-weight: bold;
    font-size: 14px;
    color: #888;
    line-height: 40px;
    text-align: center;
    display: none;
}
.NoInfoDiv.hover{
	display: block;
}


