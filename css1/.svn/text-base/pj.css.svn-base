}
.h3-my-com {
	font-size: 1.2rem;
	color: #bebebe;
	font-weight: normal;
}
.xs {
	width: 82px;height: 82px;
	/*border: dashed 1px #ccc;*/
	vertical-align: middle;
	overflow: hidden;
	float: left;
	margin-left: 20px;
}
figure{
	width: 100%;height: 100%;
	margin: 0;padding: 0;
	position: relative;
}
.anonymous p{
	height: 49px;line-height: 49px;
