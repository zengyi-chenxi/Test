	border-radius: 50%;
}
.text-pl {
	width: 100%;
	/*height: 170px;*/
	margin-top: 5px;
	float: left;
	background: #fff;
	/*padding-bottom: 20px;*/
}
.text-pl .pj-shop{
	font-size: 1.6rem;
