/**********/
body {
	margin: 0;padding: 0;
	width: 100%;
	font-family: "微软雅黑";
	/*overflow: hidden;*/
}
html{
	font-size: 62.5%;
}
li,p,ul {
	margin: 0;
	padding: 0;
}
ul,li,ol {
	list-style: none;
}
* {
	margin: 0;padding: 0;
	/*touch-action: none;*/
}
/* 重置文本格式元素 */
a {
	text-decoration: none;
	cursor: pointer;
}
a:hover {
	text-decoration: none;
}

/*顶部*/
.head-banner {
	width: 100%;
	/*height:50px;*/
	/*line-height: 50px;*/
	position: fixed;
	top: 0;left: 0;
	z-index: 99;
	background: #F5F5F5;
}
.a-fh{
	display: block;
	height: 50px;width: 45px;
	position: absolute;
	top: 0;left: 0;
	line-height: 50px;
	text-decoration: none;
}
.a-fh:hover {
	text-decoration: none;
}
.d-h-ban {
	width: 100%;height: 100%;
}
.d-h-ban p{
	font-size: 1.6rem;
    color: #2B2B2B;
    text-align: center;
    margin-right: 12px;
    line-height: 50px;
}
.d-h-ban a{
	position: absolute;
	top: 0;right: 12px;
	color: #6E717A;
	font-size: 1.5rem;
	line-height: 50px;
}

/*评价列表*/
.pj-content{
	background: #fff;
	margin-top: 50px;
}
.xj_kg{
	height: 115px;
}
.hao_ping p{
	margin: 0 !important;
}
.star_title{
	margin-bottom: 0 !important;
}
.pj-change{
	margin-top: 18px;
	margin-left: 12px;
}
.pj-change ul{
	padding: 0 !important;
	margin: 0 !important;
}
.pj-change li{
	width: 55px;height: 30px;
	line-height: 30px;
	border: 1px solid #D3D3D3;
	border-radius: 15px;
	text-align: center;
	color: #2B2B2B;
	font-size: 1.4rem;
	margin-right: 13px;
	float: left;
}
.pj-change li.liActive{
	color: #1ed3b0 !important;
	border: 1px solid #1ed3b0 !important;
}
/*评论列表样式*/
.pj-list{
	padding-top: 20px;
}
.pj-li{
	margin-bottom:23px !important;
	padding:0 12px 30px 12px !important;
	position: relative !important;
	overflow:hidden !important;
}
.pj-content-top{
	margin-bottom: 13px;
}
.yh-nickname{
	font-size: 1.3rem;
	color: #000000;
	margin-top: 3px;
	margin-bottom: 8px;
}
.pj-time{
	font-size: 1.2rem;
	color: #c0c0c0;
}
.pj-top-right{
	width: calc(100% - 4.5rem);
	margin-left: 6px;
}
.pj-top-right-xx p{
	font-size: 1.3rem;
	color: #999;
	margin-bottom: 15px;
}
.pj-top-right-xx div{
	font-size: 1.3rem;
	color: #252525;
	/*文本超过两行省略隐藏*/
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	/*-webkit-line-clamp:2;*/  
}
.pj-content-bottom-btm{
	width: 5rem;height: 2rem;
	color: #b0b0b0;
	font-size: 1.2rem;
	border-radius: 2rem;
	border: 1px solid #ededed;
	line-height: 2rem;
	text-align: center;
	vertical-align: middle;
}
.pl-icon{
	width: 1rem;height: 1.2rem;
	display: inline-block;
	vertical-align: middle;
	text-align: center;
	background: url(../img1/pl.png) no-repeat;
	background-size: 100%;
	margin-right: 3px;
	margin-top: -2px;
}
.dz-icon{
	width: 1.2rem;height: 1.3rem;
	display: inline-block;
	vertical-align: middle;
	text-align: center;
	background: url(../img1/home_praise.png) no-repeat;
	background-size: 100%;
	margin-right: 3px;
	margin-top: -4px;
}
.pj_toux{ /*头像*/
	width: 3.7rem;height: 3.7rem;
	border-radius: 50%;
	/*border: 1px solid #f00;*/
	/*background: #0ff;*/
	/*margin-left: 3px;*/
	/*margin-right: 6px;*/
	overflow: hidden;
}
.pj_toux img{
	width:100%;height:100%;
	display:block;
	margin:0 auto;
}


















.to_evaluation{
	font-size: 1.2rem;
	position: absolute;
	right:12px;top:0px;
	display: block;
	height: 50px;
}

.d-tab-xx{
