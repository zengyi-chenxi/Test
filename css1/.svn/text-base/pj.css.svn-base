}
.p-tab-xx{
	font-size:1.2em;
	color:#6e6e6e;
}


.head-pj {
	width: 100%;
	height: 40px;
	background: #fff;
	line-height: 20px;
	padding-top: 20px;
	float: left;
	border-bottom: solid 1px #eee;
}
