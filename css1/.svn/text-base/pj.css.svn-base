/*去评价页面*/
.content {
	width: 100%;
	margin-top:1rem;
	background: #fff;
}
.pj-mid{
	padding: 10px 20px 10px 20px;
	text-align: center;
}
.pj-mid h3{
	font-size: 1.6rem;
	color: #2B2B2B;
	font-weight: normal;
}
.pj-mid h3 i{
	display: inline-block;
	width: 1.7rem;height: 1.7rem;
	border-radius: 50%;
	overflow: hidden;
	vertical-align: middle;
	text-align: center;
	margin-top: -5px;
	margin-right: 6px;
}
.pj-mid h3 i.good-icon{
	background: url(../img1/good-light.png) no-repeat;
	background-size: 100%;
}
.pj-mid h3 i.middle-icon{
	background: url(../img1/middle.png) no-repeat;
	background-size: 100%;
}
.pj-mid h3 i.poor-icon{
	background: url(../img1/poor.png) no-repeat;
	background-size: 100%;
}
.mesKg{
	padding: 0 20px;
}
.mesKg textarea{
	background: none;
	margin-bottom: 0;
	font-size: 1.5rem;
	color: #2B2B2B;
	height: 11rem;
	outline: none;
	border: none;
}
.mesKg textarea::-moz-placeholder{
	font-size: 1.2rem;
	color: #bebebe;
}
.mesKg textarea::-ms-input-placeholder{
	font-size: 1.2rem;
	color: #bebebe;
}
.mesKg textarea::-webkit-input-placeholder{
	font-size: 1.2rem;
	color: #bebebe;
}
.sctp {
	width: 100%;
	height: 85px;
	margin-top: 10px;
	margin-bottom: 5px;
	clear: both;
}
.add-img {
	width: 2.4rem;height: 2.1rem;
