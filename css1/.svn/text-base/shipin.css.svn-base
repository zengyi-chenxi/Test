#address{
	height: 50px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
/*实时视频、企业资质等列表*/
.posfix{
	position: fixed;
	top: 50px;left: 0;
	z-index: 99;
}
.changer-ban{
	height: 50px;width: 100%;
	line-height: 50px;
	font-size: 1.4rem;
	background: #fff;
	padding: 0 12px;
	overflow: hidden;
}
/*文本不进行换行*/
.changer-ban ul{
	width: 100%;height: 100%;
	/*white-space: nowrap; 
    overflow-x: scroll;
    overflow-y: hidden;
    -webkit-backface-visibility: hidden;
    -webkit-perspective: 1000;
    -webkit-overflow-scrolling: touch;*/
}
/*.changer-ban ul::-webkit-scrollbar{display: none;}*/ /*隐藏滚动条*/
.changer-ban li{
	width: 24%;
	/*width: 23%;*/
	height: 50px;
	display: inline-block;
	font-size: 1.4rem;
	color: #646464;
	text-align: center;
}
.changer-ban li p{
	display: inline-block;
	padding: 0 5px;
}
.changer_active{
	position: relative;
	color: #25dbaa;
}
.changer_active::after,.changer_active::before{
	content: "";
	width: 100%;height: 3px;
	background: #25dbaa;
	border-radius: 20px;
	position: absolute;
	bottom: 3px;left: 0;
}
/*实时监控*/
.realTime_monitoring{
	background: #fff;
	margin-top: 5px;
}
.infor_title{
	width:96%;
	height:auto;
	margin:0 auto;
	color: #2b2b2b;
	font-size: 1.6rem;
	padding:5px 0;
	font-weight:bold;
}
.monitoring{
	padding: 12px 12px;
	/*text-align: center;*/
	white-space: nowrap; /*文本不进行换行*/
    overflow-x: scroll;
    overflow-y: hidden;
    -webkit-backface-visibility: hidden;
    -webkit-perspective: 1000;
    -webkit-overflow-scrolling: touch;
}
.monitoring::-webkit-scrollbar{display: none;} /*隐藏滚动条*/
._flex{
	display: flex;
	display: -webkit-flex;
	flex-direction: row;
	justify-content: space-between;
}
.monitoring_item{
	width: 8.8rem;
	font-size: 1.3rem;
	display: inline-block;
	margin-right: 10px;
}
.monitoring_item:last-child{
	margin-right: 0;
}
.spKg{
	width: 8.8rem;height: 6.5rem;
	overflow: hidden;
	position: relative;
}
.spKg img{
	width: 100%;height: 100%;
}
.spKg span{
	position: absolute;
	background: #1FD3B3;
	color: #fff;
	top: 0;right: 10px;
	padding: 0 4px;
}
.monitoring_item p{
	font-size: 1.3rem;
	color: #646464;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	text-align: center;
}
