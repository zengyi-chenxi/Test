body {
	margin: 0 auto;
	-webkit-overflow-scrolling: touch;
	overflow-scrolling: touch;
	background: #F5F5F5;
}

ul,li {
	list-style: none;
}

* {
	margin: 0;
	padding: 0;
	list-style: none;
	font-family: "微软雅黑";
}
a{
	text-decoration: none;
}
a:visited , a:focus ,a:hover{
	text-decoration:none !important;
}
._left{
	float: left;
}
html{
	font-size: 62.5%;
}

/*顶部*/
.head-banner {
	width: 100%;height:50px;
	line-height: 50px;
	position: fixed;
	top: 0;left: 0;
	z-index: 99;
	background: linear-gradient(to right, #24dea1, #1fd3b3 40%);
}
.a-fh{
	display: block;
	height: 50px;width: 45px;
	position: absolute;
	top: 0;left: 0;
}
.d-h-ban {
	width: 100%;height: 100%;
}
.d-h-ban p{
	font-size: 1.6rem;
    color: #fff;
    text-align: center;
}


/*巡查页面--信息*/
.show-information{
	width:100%;
	overflow: hidden;
	/*margin-top:50px;*/
	background:#F5F5F5;
}
/*视频*/
.d-video {
	width: 100%;
	overflow: hidden;
	position: relative;
}
.d-video-img{
	background: url(../img/timg2.gif) no-repeat center center;
	background-size: 100%;
}
/*巡检、语音部分*/
.red_article{
	height: 2px;
	background: linear-gradient(to right, #24dea1, #1fd3b3 40%);
}
.vedio_change{
	height: 55px;
	background: #f2f2f2;
	/*border-bottom: 2px solid #b7b7b7;*/
	position: relative;
	margin-top: -7px;
}
.vedio_change li{
	float: left;
	width: 49.8%;height: 55px;
	cursor: pointer;
	text-align: center;
	color: rgb(100,100,100);
	position: relative;
}
.li_xian{
	float: right;
	font-size:0;
	width: 1px;height: 43px;
	margin-top: 5px;
	border-right: 1px dashed #aeaeae;
}
.vedio_change li img{
	margin-top: 13px;
	width: 25px;
}
.vedio_change li p{
	margin-top: -4px;
	font-size: 1.2rem;
}
/*历史录像*/
.watch_history{
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 1.3rem;
	color: rgb(185,185,185);
	background: #fff;
	border-bottom: 1px solid #d7d7d7;
}
.watch_history img{
	width: 15px;
	margin-top: -4px;
	margin-right: 4px;
	vertical-align: middle;
}
/*点赞收藏*/
.d-introduce {
	width: 100%;
	background: #fff;
	margin-bottom: 5px;
}
.left-introduce {
	width: 100%;
	padding-top: 7px;
}
.shop_name{
	font-size: 1.6rem;
	color: rgb(43,43,43);
	margin: 0 12px;
	max-width: calc(100% - 200px);
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.sc{
	display: block;
	width: 1.4rem;height: 1.2rem;
	margin-top: 1px;
}
.sc img,.dz img{
	width: 100%;
}
.dz{
	display: block;
	width: 1.3rem;height: 1.3rem;
}
.num-sc,.num-dz{
	float:left;
	font-size:1.2rem;
	color:rgb(153,153,153);
	margin-left: 5px;
}
.p2-left-introduce {
	float: left;
	margin-left: 16px;
	font-size: 1.2em;
}
.shop_description{
	background: #fff;
	height: 50px;
	line-height: 50px;
	padding: 0 12px;
}
.shop_description li{
	width: 33.33%;height: 100%;
	float: left;
	color: #999;
	font-size: 1.2em;
}
.shop_type{
	display: inline-block;
	width: 26px;height: 26px;
	border-radius: 50%;
	margin-top: 10px;
	overflow: hidden;
}
.shop_type img{
	width: 100%;height: 100%;
	text-align: center;
	vertical-align: top;
}
.shop_type_name{
	height: 24px;
	line-height: 27px;
	background: #ff6d70;
	border-radius: 4px;
	text-align: center;
	color: #fff;
	margin-top: 12px;margin-left: 2px;
	padding: 0 3px;
	font-size: 1.3rem;
	max-width: calc(100% - 32px);
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.jbkg{
	display: none;
}
.jbkg p{
	font-size: 1.4rem;
	color: #6E717A;
	margin-right: 12px;
	padding-left: 8px;
}
.jbkg p img{
	width: 1.7rem;
	vertical-align: middle;
	margin-top: -3px;
	margin-right: 5px;
}
/*修改量化等级图标*/
