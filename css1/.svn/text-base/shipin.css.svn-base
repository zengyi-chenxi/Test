}
.health_icon{
	background: url(../img/health-icon.png) no-repeat center center;
	background-size: 100%;
}
.qualification_icon{
	background: url(../img/qualification_icon.png) no-repeat center center;
	background-size: 100%;
}
.introduction_icon{
	background: url(../img/introduction_icon.png) no-repeat center center;
	background-size: 100%;
}
.VR_view_icon{
	background: url(../img/VR_view_icon.png) no-repeat center center;
	background-size: 100%;
}
.jobtime_icon{
	background: url(../img/jobtime_icon.png) no-repeat center center;
	background-size: 100%;
}
.swiper-container {
	width: 100%;height: 6.5rem;
	text-align: center;
	margin-top: 8px;
	overflow: hidden;
}
figure{
	padding: 0;
	margin: 0;
}
.swiper-slide {
	opacity: 1;
	-webkit-transition: 300ms;
	-moz-transition: 300ms;
	-ms-transition: 300ms;
	-o-transition: 300ms;
	transition: 300ms;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}
.swiper-slide-visible {
	opacity: 1;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}
.swiper-slide-active {
	top: 0;
	opacity: 1;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}
.swiper-slide .title {
	font-style: italic;
	margin-top: 0px;
	margin-bottom: 0;
}
.swiper-pagination-switch {
	display: inline-block;
	width: 10px;
	height: 10px;
	border-radius: 8px;
	background: #aaa;
	margin-right: 8px;
	cursor: pointer;
	-webkit-transition: 300ms;
	-moz-transition: 300ms;
	-ms-transition: 300ms;
	-o-transition: 300ms;
	transition: 300ms;
	opacity: 0;
	position: relative;
	top: -50px;
}
.swiper-visible-switch {
	opacity: 1;
	top: 0;
	background: #fff;
}
.swiper-active-switch {
	background: #fff;
}