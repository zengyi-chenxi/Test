	margin-top: 9px;margin-left: 5px;
	float: left;
	background:url(../img/lha.png) no-repeat;
	background-size: 100%;
}
.right-introduceb {
	width: 32px;height: 32px;
	overflow: hidden;
	margin-top: 9px;margin-left: 5px;
	float: left;
	background:url(../img/lhb.png) no-repeat;
	background-size: 100%;
}
.right-introducec {
	width: 32px;height: 32px;
	overflow: hidden;
	margin-top: 9px;margin-left: 5px;
	float: left;
	background:url(../img/lhc.png) no-repeat;
	background-size: 100%;
}
.right-introduced {
	width: 32px;height: 32px;
	overflow: hidden;
	margin-top: 9px;margin-left: 5px;
	float: left;
	background:url(../img/lhd.png) no-repeat;
	background-size: 100%;
